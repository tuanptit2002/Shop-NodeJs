"version": 2,
   "builds": [
       {
           "src": "package.json",
           "use": "@vercel/static-build"
       },
       {
           "src": "src/main.ts",
  "use": "@vercel/node",
           "config": { "includeFiles": "./**" }
       }
   ],
   "routes": [
       {
           "src": "/(.*)",
           "dest": "src/index.js",
           "methods": [
               "GET",
               "OPTIONS",
               "POST",
               "PUT",
               "PATCH",
               "DELETE",
               "HEAD"
           ]
       }
   ]
}
